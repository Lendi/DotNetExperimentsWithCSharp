<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Admin.master.cs" Inherits="WebApplication2.Admin" %>
<!DOCTYPE html>
<html lang="en">
<head id="Head1" runat="server">
    <meta charset="utf-8" />
    <title><%: Page.Title %> - My ASP.NET Application</title>
    <asp:PlaceHolder ID="PlaceHolder1" runat="server">     
          <%: Scripts.Render("~/bundles/modernizr") %>
    </asp:PlaceHolder>  
    <webopt:BundleReference ID="BundleReference1" runat="server" Path="~/Content/css" /> 
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <asp:ContentPlaceHolder runat="server" ID="HeadContent" />
    <style type="text/css">
        .auto-style1
        {
            width: 82%;
            height: 560px;
        }
    </style>
</head>
<body>
    <form id="Form2" runat="server">
    <asp:ScriptManager ID="ScriptManager1" runat="server">
        <Scripts>
            <%--Framework Scripts--%>
            <asp:ScriptReference Name="MsAjaxBundle" />
            <asp:ScriptReference Name="jquery" />
            <asp:ScriptReference Name="jquery.ui.combined" />
            <asp:ScriptReference Name="WebForms.js" Assembly="System.Web" Path="~/Scripts/WebForms/WebForms.js" />
            <asp:ScriptReference Name="WebUIValidation.js" Assembly="System.Web" Path="~/Scripts/WebForms/WebUIValidation.js" />
            <asp:ScriptReference Name="MenuStandards.js" Assembly="System.Web" Path="~/Scripts/WebForms/MenuStandards.js" />
            <asp:ScriptReference Name="GridView.js" Assembly="System.Web" Path="~/Scripts/WebForms/GridView.js" />
            <asp:ScriptReference Name="DetailsView.js" Assembly="System.Web" Path="~/Scripts/WebForms/DetailsView.js" />
            <asp:ScriptReference Name="TreeView.js" Assembly="System.Web" Path="~/Scripts/WebForms/TreeView.js" />
            <asp:ScriptReference Name="WebParts.js" Assembly="System.Web" Path="~/Scripts/WebForms/WebParts.js" />
            <asp:ScriptReference Name="Focus.js" Assembly="System.Web" Path="~/Scripts/WebForms/Focus.js" />
            <asp:ScriptReference Name="WebFormsBundle" />
            <%--Site Scripts--%>

        </Scripts>
    </asp:ScriptManager>
        <asp:LinkButton ID="LinkButton1" runat="server" CssClass="float-right" 
            OnClick="LinkButton1_Click" CausesValidation="False">Logout</asp:LinkButton>
    <header >
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">
                    Asset Management Portal</p>
            </div>
            <div class="float-right">
                <section id="login">
                   
                </section>
                <nav>
                    <ul id="menu">
                        <li><a id="A2" runat="server" href="~/Account/Admin/Admin_Home.aspx">Home</a></li>
                        <li><a id="A3" runat="server" href="~/Account/Admin/Admin_Home.aspx">About</a></li>
                        <li><a id="A4" runat="server" href="~/Account/Admin/Admin_Home.aspx">Contact</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <div id="body">
        <asp:ContentPlaceHolder runat="server" ID="FeaturedContent" />
        <section class="Float-right">
             <section id="xyz" class="float-left">
        <section style="border-right: solid 2px #c8c8c8;padding-top:30px">
        
                    <h2 style="padding-left:100px">Employee Menu</h2>
                   
                    <table class="auto-style1" style="text-align:center">
                                <tr>
                                    <td><asp:LinkButton ID="ViewUsers" runat="server" 
                                PostBackUrl="~/Account/Admin/Admin_ViewUsers.aspx" CausesValidation="False">View Users</asp:LinkButton></td>
                                </tr>
                                <tr>
                                    <td><asp:LinkButton ID="CreateRequest" runat="server" 
                                PostBackUrl="~/Account/Admin/Admin_CreateUser.aspx" CausesValidation="False">Create User</asp:LinkButton></td>
                                </tr>
                                <tr>
                                    <td><asp:LinkButton ID="ViewRequest" runat="server" 
                                PostBackUrl="~/Account/Admin/Admin_ModifyUser.aspx" CausesValidation="False">Modify User</asp:LinkButton></td>
                                </tr>
                                
                            </table>
                        </section>
    </section>
        </section>
        <section class="Float-left">
        <fieldset style="padding-left:100px;padding-top:100px">
        <legend></legend>
            <asp:ContentPlaceHolder runat="server" ID="ContentPlaceHolder2" />
            </fieldset>

        </section>
    </div>
    <footer>
        <div class="content-wrapper">
            <div class="float-right">
                <p>&copy; <%: DateTime.Now.Year %> - My ASP.NET Application</p>
            </div>
        </div>
    </footer>
    </form>
</body>
</html>